    "react-dom": "^18.3.0",
    "react-github-calendar": "^4.1.6",
    "react-simple-maps": "^3.0.0",
    "react-slick": "^0.29.0",
    "react-tooltip": "^5.10.1",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "sass": "^1.32.8",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.6",
